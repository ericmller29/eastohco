<head>
    <title>EastOh.Co</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    
    <link rel="stylesheet" href="//fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,600italic,700italic,800italic,400,300,600,700,800|Roboto+Slab:400,100,300,700">
    <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="/css/screen.css">

    <link rel="icon" type="image/png" href="/img/favicon-32x32.png?v=2" sizes="32x32" />
    <link rel="icon" type="image/png" href="/img/favicon-16x16.png?v=2" sizes="16x16" />
</head>
<body>
    <nav class="navigation">
        <div class="container">
            <ul class="navigation__list">
                {{#each navItems}}
                    {{#if_eq ../activeNav this.title}}
                    <li class="navigation__list_item active"><a href="/{{this.path}}">{{this.title}}</a></li>
                    {{else}}
                    <li class="navigation__list_item"><a href="/{{this.path}}">{{this.title}}</a></li>
                    {{/if_eq}}
                {{/each}}
            </ul>
        </div>
    </nav>
    <div class="page-wrap">
        <header class="header">
            <h1 class="logo"><a href="/" alt="Homepage" title="Homepage"><img src="/img/logo.png" width="150"></a></h1>
        </header>
        <div id="js__main_content">